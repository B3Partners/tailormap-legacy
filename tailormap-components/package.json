{
  "name": "tailormap-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm-watch serve",
    "serve": "ng serve",
    "build": "ng build",
    "install-all": "npm run merge-third-party-dependencies && npm install",
    "merge-third-party-dependencies": "node bin/merge-dependencies.js",
    "build-prod": "npm run merge-third-party-assets && npm run ng -- build --configuration production --output-hashing=none",
    "merge-third-party-assets": "node bin/merge-assets-angular.js",
    "test": "ng test --project=core",
    "test:ci": "ng test --project=core --code-coverage --no-watch --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint-core": "ng lint --project=core",
    "lint-fix": "ng lint --fix",
    "lint-fix-core": "ng lint --fix --project=core",
    "stylelint": "stylelint **/*.css",
    "stylelint-fix": "stylelint **/*.css --fix",
    "e2e": "ng e2e",
    "ng-openapi-gen": "ng-openapi-gen",
    "generate": "ng-openapi-gen -c generateConfig.json",
    "test-circular": "dpdm -T ./projects/bridge/src/main.ts && dpdm -T ./projects/core/src/index.ts",
    "generate-component": "ng g cs --project=core",
    "generate-service": "ng g ss --project=core",
    "generate-http-service": "ng g ss --project=core --isDataService=true"
  },
  "watch": {
    "serve": {
      "patterns": [
        "../../tailormap-frontend/projects"
      ],
      "extensions": "ts,css,html"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.1.4",
    "@angular/cdk": "12.1.4",
    "@angular/common": "12.1.4",
    "@angular/compiler": "12.1.4",
    "@angular/core": "12.1.4",
    "@angular/elements": "12.1.4",
    "@angular/forms": "12.1.4",
    "@angular/material": "12.1.4",
    "@angular/material-moment-adapter": "12.1.4",
    "@angular/platform-browser": "12.1.4",
    "@angular/platform-browser-dynamic": "12.1.4",
    "@angular/router": "12.1.4",
    "@ngrx/effects": "^12.3.0",
    "@ngrx/store": "^12.3.0",
    "@ngrx/store-devtools": "^12.3.0",
    "@tailormap/core-components": "0.0.11",
    "@tailormap/models": "0.0.2",
    "@tailormap/shared": "0.0.14",
    "@types/wellknown": "^0.5.1",
    "document-register-element": "^1.14.10",
    "dpdm": "^3.7.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ng-openapi-gen": "^0.17.2",
    "rxjs": "6.6.7",
    "tslib": "^2.3.0",
    "wellknown": "^0.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.4",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.1.4",
    "@angular/compiler-cli": "12.1.4",
    "@angular/language-service": "12.1.4",
    "@ngneat/spectator": "^8.0.1",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "codelyzer": "^6.0.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "36.0.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-rxjs": "^3.3.5",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.1.2",
    "npm-watch": "0.11.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-node": "~10.1.0",
    "typescript": "~4.3.5"
  }
}
