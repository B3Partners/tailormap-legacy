{
  "name": "tailormap-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run ng -- build --configuration production --output-hashing=none",
    "test": "ng test --project=core",
    "test:ci": "ng test --project=core --code-coverage --no-watch --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint-core": "ng lint --project=core",
    "lint-fix": "ng lint --fix",
    "lint-fix-core": "ng lint --fix --project=core",
    "stylelint": "stylelint **/*.css",
    "stylelint-fix": "stylelint **/*.css --fix",
    "e2e": "ng e2e",
    "ng-openapi-gen": "ng-openapi-gen",
    "generate": "ng-openapi-gen -c generateConfig.json",
    "test-circular": "dpdm -T ./projects/bridge/src/main.ts && dpdm -T ./projects/core/src/index.ts",
    "generate-component": "ng g cs --project=core",
    "generate-service": "ng g ss --project=core",
    "generate-http-service": "ng g ss --project=core --isDataService=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.3",
    "@angular/cdk": "12.0.3",
    "@angular/common": "12.0.3",
    "@angular/compiler": "12.0.3",
    "@angular/core": "12.0.3",
    "@angular/elements": "12.0.3",
    "@angular/forms": "12.0.3",
    "@angular/material": "12.0.3",
    "@angular/material-moment-adapter": "12.0.3",
    "@angular/platform-browser": "12.0.3",
    "@angular/platform-browser-dynamic": "12.0.3",
    "@angular/router": "12.0.3",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@tailormap/core-components": "0.0.4",
    "@tailormap/shared": "0.0.3",
    "@types/wellknown": "^0.5.1",
    "document-register-element": "^1.14.10",
    "dpdm": "^3.6.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ng-openapi-gen": "^0.16.0",
    "rxjs": "6.6.7",
    "tslib": "^2.1.0",
    "wellknown": "^0.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.3",
    "@angular-eslint/builder": "12.1.0",
    "@angular-eslint/eslint-plugin": "12.1.0",
    "@angular-eslint/eslint-plugin-template": "12.1.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "12.1.0",
    "@angular/cli": "12.0.3",
    "@angular/compiler-cli": "12.0.3",
    "@angular/language-service": "12.0.3",
    "@ngneat/spectator": "^7.0.0",
    "@types/jasmine": "~3.6.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "codelyzer": "^6.0.2",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-rxjs": "^3.1.0",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^12.0.3",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.2.4"
  }
