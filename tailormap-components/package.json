{
  "name": "tailormap-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run ng -- build --prod --output-hashing=none",
    "test": "ng test --project=core",
    "test:ci": "ng test --project=core --code-coverage --no-watch --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint-core": "ng lint --project=core",
    "lint-fix": "ng lint --fix",
    "lint-fix-core": "ng lint --fix --project=core",
    "stylelint": "stylelint **/*.css",
    "stylelint-fix": "stylelint **/*.css --fix",
    "e2e": "ng e2e",
    "ng-openapi-gen": "ng-openapi-gen",
    "generate": "ng-openapi-gen -c generateConfig.json",
    "test-circular": "dpdm -T ./projects/bridge/src/main.ts && dpdm -T ./projects/core/src/index.ts",
    "generate-component": "ng g cs --project=core",
    "generate-service": "ng g ss --project=core",
    "generate-http-service": "ng g ss --project=core --isDataService=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/cdk": "~11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/elements": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/material": "~11.1.1",
    "@angular/material-moment-adapter": "~11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@types/wellknown": "^0.5.1",
    "document-register-element": "^1.14.10",
    "dpdm": "^3.6.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "ng-openapi-gen": "^0.15.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0",
    "wellknown": "^0.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.1",
    "@angular/cli": "~11.2.1",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.1.2",
    "@ngneat/spectator": "^7.0.0",
    "@types/jasmine": "~3.6.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.31",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.2.2",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.34.7",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
